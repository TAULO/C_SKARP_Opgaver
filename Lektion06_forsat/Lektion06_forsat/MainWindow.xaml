<Window x:Class="Lektion06_forsat.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lektion06_forsat"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button x:Name="vest" DockPanel.Dock="Left" Click="Button_Click">Vest</Button>
            <Button x:Name="nord" DockPanel.Dock="Top" Click="Button_Click">Nord</Button>
            <Button x:Name="øst" DockPanel.Dock="Right" Click="Button_Click">Øst</Button>
            <Button x:Name="syd" DockPanel.Dock="Bottom" Click="Button_Click">Syd </Button>
            <TextBox x:Name="textBox" HorizontalAlignment="Center"></TextBox>

        </DockPanel>
    </Grid>
</Window>
